    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run :: GRPC",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "env": {},
            "args": [
                "--hostname",
                "localhost",
                "--port",
                "3000",
                "--basepath",
                "../.."
            ],
            "envFile": "${workspaceFolder}/.env",
            "internalConsoleOptions": "openOnSessionStart",
            "showLog": true,
            "output": "debug"
        }
    ]
}
